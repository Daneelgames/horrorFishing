    - "\u0441\u043C\u043E\u0442\u0440\u0438 \u043D\u0430\u0432\u0435\u0440\u0445\u0443"
    - "\u0438\u0449\u0438 \u0432\u043D\u0438\u0437\u0443"
    - "\u0441\u0442\u0440\u0435\u043B\u044F\u0439 \u0432 \u0433\u043E\u043B\u043E\u0432\u0443,
      \u0431\u0435\u0439 \u0432 \u0442\u0435\u043B\u043E"
    - "\u0436\u0440\u0438 \u043E\u0440\u0443\u0436\u0438\u044F"
    - "\u043E\u043D\u0438 \u0442\u0435\u0431\u044F \u0441\u043B\u044B\u0448\u0430\u0442"
    - "\u0442\u044B \u0433\u0440\u043E\u043C\u043A\u043E \u0431\u0435\u0433\u0430\u0435\u0448\u044C"
    - "\u0442\u044B \u0433\u0440\u043E\u043C\u043A\u043E \u0441\u0442\u0440\u0435\u043B\u044F\u0435\u0448\u044C"
    - "\u0442\u044B \u0442\u0438\u0445\u043E \u0431\u044C\u0435\u0448\u044C"
    - "\u0431\u0435\u0439 \u0432\u043F\u0435\u0440\u0435\u0434"
    - "\u0431\u0435\u0436\u0430\u0442\u044C \u043D\u0435 \u0441\u0442\u044B\u0434\u043D\u043E"
    - "\u0435\u0441\u043B\u0438 \u043D\u043E\u0433\u0438 \u0446\u0435\u043B\u044B"
    - "\u043B\u0435\u0447\u0438\u0441\u044C"
    - "\u0425\u0412\u0410\u0422\u0418\u0422 \u0423\u0411\u0418\u0412\u0410\u0422\u042C
      \u0412\u0421\u0415\u0425"
    - "\u043A\u0440\u0430\u0434\u0438\u0441\u044C"
    - "\u043F\u0443\u0448\u043A\u0438 \u043B\u043E\u043C\u0430\u044E\u0442\u0441\u044F"
    - "\u0443\u0431\u0435\u0433\u0430\u0439"
    - "\u0443\u0447\u0438\u0441\u044C \u0438\u0437\u0431\u0435\u0433\u0430\u0442\u044C
      \u0434\u0440\u0430\u043A\u0438"
    - "\u043D\u0430 \u043A\u043E\u0440\u0442\u0430\u0445 \u0442\u0435\u0431\u044F
